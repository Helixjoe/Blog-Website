<%- include('partials/header'); -%>
    <h1>Compose</h1>
    <form action="/compose" method="post">
        <div class="form-group">
            <label>Title</label>
            <input type="text" name="contentTitle" class="form-control" required>
            <label>Post</label>
            <textarea type="text" name="contentPost" class="form-control" rows="3" cols="30" required></textarea>
        </div>
        <button type="submit" name="submit" class="btn btn-primary">Publish</button>
    </form>
    <%- include('partials/footer'); -%>